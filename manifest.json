{
  "name": "SmartRent Dark Mode",
  "description": "View SmartRent in dark mode.",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "./scripts/background.js"
  },
  "content_scripts": [{
    "css": [ "./styles/supportRent.css" ],
    "js/js": [ "./js/content.js" ],
    "matches": ["https://support.qa-smartrent.com/*"]
  }],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/sr-logo16.png",
      "32": "./images/sr-logo32.png",
      "48": "./images/sr-logo48.png",
      "128": "./images/sr-logo128.png"
    }
  },
  "icons": {
    "16": "./images/sr-logo16.png",
    "32": "./images/sr-logo32.png",
    "48": "./images/sr-logo48.png",
    "128": "./images/sr-logo128.png"
  }
}

